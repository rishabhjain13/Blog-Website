<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title>My Blog</title>
  </head>
  <body>
    <%- include('partials/header'); -%>
    <h1>Home</h1>
    <p><%=homeStartingContent%></p>

    <%  posts.forEach(function(entry){  %>
      <h1><%= entry.title %></h1>
      <p><%= entry.body.substring(0,100) + " ..." %><a href="/posts/<%=entry.title%>">Read More</a></p>
    <%  });  %>



    <%- include('partials/footer'); -%>
  </body>
</html>
